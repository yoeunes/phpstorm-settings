<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Comment with Line Comment">
        <Item>
          <IdeaActionID>CommentByLineComment</IdeaActionID>
          <ShortCut>'⌘/' or '⌘NumPad /'</ShortCut>
          <Description>Comment with Line Comment</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Reference">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'⌥⇧⌘C'</ShortCut>
          <Description>Copy Reference</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Relative Path">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'⌥⇧⌘C'</ShortCut>
          <Description>Copy Relative Path</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>168</Count>
        </Item>
      </Statistic>
      <Statistic Action="Exit Full Screen">
        <Item>
          <IdeaActionID>ToggleFullScreen</IdeaActionID>
          <ShortCut>'⌃⌘F'</ShortCut>
          <Description>Exit Full Screen</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Navigation Bar">
        <Item>
          <IdeaActionID>ShowNavBar</IdeaActionID>
          <ShortCut>'⌘↑' or '⌥↖'</ShortCut>
          <Description>Jump to Navigation Bar</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>159</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'UpdateContactEmailAction.php'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'UpdateContactEmailAction.php'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'UpdateUrlActionTest and 16 more'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'UpdateUrlActionTest and 16 more'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'UpdateUrlActionTest'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌘R'</ShortCut>
          <Description>Rerun 'UpdateUrlActionTest'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>36</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>